{
  "require": {
    "slim/slim": "^4.0",
    "slim/psr7": "^1.5",
    "php-di/php-di": "^6.0",
    "jenssegers/blade": "^1.4",
    "php": "^8.2",
    "ext-curl": "*",
    "ext-json": "*",
    "php-di/slim-bridge": "^3.4",
    "doctrine/orm": "^2.11",
    "symfony/cache": "^5.4",
    "vlucas/phpdotenv": "^5.6",
    "doctrine/migrations": "^3.8",
    "firebase/php-jwt": "^6.10",
    "elasticsearch/elasticsearch": "^8.15",
    "doctrine/dbal": "^3.9"
  },
  "autoload": {
    "files": [
      "./app/helpers.php"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "pestphp/pest-plugin": true
    }
  },
  "require-dev": {
    "pestphp/pest": "^1.23"
  }
}
